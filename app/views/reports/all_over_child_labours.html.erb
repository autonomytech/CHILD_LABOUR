<div class="add-btn">
<%= link_to 'Download Pdf', download_pdf_reports_path(type: 'all_over_child_labours'), class: 'btn btn-primary btn-xs', target: "_blank" %>
</div>
<div class="row">
  <div class="col-md-1"></div>
  <div class="col-md-10">
  <table class="table table-striped table-hover table-bordered">
  <thead>
    <tr>
      <th>Sr. No.</th>
      <th>Year</th>
      <th>Total FIR</th>
      <th>Victam in jail</th>
      <th>Free child labour </th>
    </tr>
  </thead>
  <tbody>
  	<% @years.each_with_index do |year, i|%>
    <tr>
    	<td><%= i+1 %></td>
    	<td><%= year %></td>
    	<td><%= total_fir(year) %></td>
    	<td><%= victam_in_jail_cl(year) %></td>
    	<td><%= free_child_labour(year) %></td>
    </tr>
    <% end %>
  </tbody>
  </table>
  </div>
</div>
